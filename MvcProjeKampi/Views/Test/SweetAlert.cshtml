
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<button class="btn btn-primary" id="btn1">Button 1</button>
<button class="btn btn-warning" id="btn2">Button 2</button>
<button class="btn btn-danger" id="btn3">Button 3</button>

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.slim.min.js"></script>
<script>
    $('#btn1').click(function () {
        swal("MVC Proje Kampı !");
    });</script>
<script>
    $('#btn2').click(function () {
        swal("Mvc Proje Kampı ", "100 derslik seri tamamlandı.");
    });</script>
<script>
    $('#btn3').click(function () {
        swal("Başlık", "EKleme yapıldı", "success");
    });
</script>

